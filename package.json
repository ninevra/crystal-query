{
  "name": "query-filter",
  "version": "0.1.0",
  "description": "A simple query language with customizable semantics",
  "author": "ninevra",
  "license": "ISC",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "type": "module",
  "exports": {
    "import": "./src/index.js"
  },
  "scripts": {
    "test": "c8 ava",
    "test:debug": "ava debug --break",
    "format": "prettier --write .",
    "format:watch": "onchange '**/*.js' -- prettier --write {{changed}}",
    "snaps:update": "ava --update-snapshots"
  },
  "dependencies": {
    "parsimmon": "^1.13.0"
  },
  "devDependencies": {
    "ava": "^3.9.0",
    "c8": "^7.2.0",
    "onchange": "^7.0.2",
    "prettier": "^2.0.5"
  },
  "ava": {
    "verbose": true,
    "nonSemVerExperiments": {
      "likeAssertion": true
    },
    "snapshotDir": "snapshots"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  }
}
